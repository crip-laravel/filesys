{
  "name": "crip-laravel/filesys",
  "description": "Laravel - Filemanager extension",
  "require": {
    "crip/core": "^1.0",
    "illuminate/support": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
    "intervention/image": "^2.3",
    "php": ">=7.0"
  },
  "require-dev": {
    "league/flysystem-vfs": "^1.0",
    "orchestra/testbench": "^3.4",
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "^0.9.9"
  },
  "autoload": {
    "psr-4": {
      "Crip\\Filesys\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crip\\Filesys\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "homepage": "https://github.com/crip-laravel/filesys",
  "license": "MIT",
  "authors": [
    {
      "name": "Igors Krasjukovs",
      "email": "tahq69@gmail.com",
      "homepage": "http://crip.lv",
      "role": "Lead Developer"
    }
  ],
  "minimum-stability": "dev"
}
